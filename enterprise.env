# FlowStack Enterprise Configuration
# Copy these to core/packages/server/.env

# Authentication
FLOWISE_JWT_SECRET=your-secret-jwt-string-12345
FLOWISE_SESSION_SECRET=my_secret_session_key

# Database (using existing SQLite)
DATABASE_PATH=./flowise.db

# Server
PORT=3000

# CORS
CORS_ORIGINS=http://localhost:3001,http://localhost:8080

# FlowStack Enterprise Features
ENABLE_ENTERPRISE=true
OFFLINE=true 