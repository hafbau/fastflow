# FlowStack Development Environment Variables
# Copy this file to .env.dev and update with your values

# Database Configuration
DATABASE_PASSWORD=devpassword

# Development Credentials
FLOWISE_USERNAME=admin@dev.local
FLOWISE_PASSWORD=devpassword

# API Keys (Optional - add your own for testing)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=

# External Port (if 3000 is already in use)
EXTERNAL_PORT=3000

# Node Environment
NODE_ENV=development

# Secret Keys
FLOWISE_SECRETKEY_OVERWRITE=devSecretKey

# File Upload Limit
FLOWISE_FILE_SIZE_LIMIT=100mb

# Execution Mode
EXECUTION_MODE=main

# Queue Provider (memory for dev, redis for testing scaling)
QUEUE_PROVIDER=memory

# Redis URL (if using redis queue provider)
REDIS_URL=redis://redis:6379